*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]
:PROXY - [0:0]
-A INPUT -i lo -j ACCEPT
-A OUTPUT -o lo -j ACCEPT
-A OUTPUT -m owner --owner-uid snihdnatd -j PROXY
-A OUTPUT -m owner --owner-uid snihdnatd -j REJECT
{% for x in whitelist.dests %}
-A PROXY -p tcp -d {{ x }} --dport 80 -j ACCEPT
-A PROXY -p tcp -d {{ x }} --dport 443 -j ACCEPT
{% endfor %}
COMMIT
